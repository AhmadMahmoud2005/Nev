func main() {
    
    // to declare a mutable variable
    type name = val
    let name = val
    // to declare an immutable
    const str name = val // size auto string 
    const int name = val // size auto integer 
    const bool name = val // size auto boolean 
    const name = val

    /* data types are
    num: normal number
    str: text
    char: character
    bool: boolean
    i8 i16 i32 i64: integer number
    u8 u16 u32 u64: unsigned integer number
    f32 f64: floating point number*/

    // to declare nullable variable u should use ? after type
    let? name = "youssefully";
    u8? age = 23

}